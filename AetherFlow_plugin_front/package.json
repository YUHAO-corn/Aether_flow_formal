{
  "name": "mystical-sidebar-extension",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:extension": "node scripts/build-extension.js",
    "preview": "vite preview",
    "test": "jest",
    "test:watch": "NODE_OPTIONS=--experimental-vm-modules jest --watch",
    "test:coverage": "NODE_OPTIONS=--experimental-vm-modules jest --coverage",
    "error-monitor": "node scripts/start-error-monitor.js",
    "dev:monitor": "npm run error-monitor && npm run dev",
    "test:auto": "node scripts/auto-test.js",
    "test:e2e": "npm run dev & npm run test:auto",
    "dev:extension": "vite build --watch",
    "dev:extension:watch": "node scripts/dev-extension.js",
    "test:extension": "jest --testMatch='**/*.test.js'",
    "dev:hot": "node scripts/dev-hot-reload.js",
    "run:extension": "node scripts/run-extension.js",
    "dev:with-reload": "node scripts/dev-with-reload.js",
    "check:dist": "node scripts/check-dist.js",
    "build:dev": "vite build --config vite.config.dev.js",
    "clean:dist": "rm -rf dist && mkdir dist"
  },
  "dependencies": {
    "autoprefixer": "^10.4.16",
    "axios": "^1.8.2",
    "framer-motion": "^10.16.4",
    "postcss": "^8.4.31",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.11.0",
    "react-particles": "^2.12.2",
    "react-type-animation": "^3.2.0",
    "react-use": "^17.4.0",
    "tailwindcss": "^3.3.3",
    "tsparticles-engine": "^2.12.0",
    "tsparticles-slim": "^2.12.0"
  },
  "devDependencies": {
    "@babel/core": "^7.22.5",
    "@babel/preset-env": "^7.22.5",
    "@babel/preset-react": "^7.22.5",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react": "^4.0.3",
    "babel-jest": "^29.5.0",
    "body-parser": "^1.20.3",
    "chalk": "^5.4.1",
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "http": "^0.0.1-security",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "vite": "^4.4.5",
    "vite-plugin-web-extension": "^4.4.3",
    "web-ext": "^8.4.0",
    "ws": "^8.18.1"
  }
}
